{
  "general": {
    "app_name": "goinkblog",
    "version": "v1.0.0",
    "debug": false,
    "disable_swagger": false,
    "disable_print_config": false,
    "pprof_addr": "",
    "http": {
      "addr": ":52443",
      "shutdown_timeout": 30,
      "max_content_length": 67108864,
      "read_timeout": 60,
      "write_timeout": 0,
      "idle_timeout": 120,
      "cert_file": "",
      "key_file": ""
    },
    "admin": {
      "id": 1,
      "username": "Admin",
      "email": "admin@email.com",
      "password": "admin123",
      "avatar": "/pic/avatars/admin.jpg",
