    "cache": {
      "delimiter": ":",
      "redis": {
        "addr": "127.0.0.1:6379",
        "db": 0,
        "username": "",
        "password": ""
      }
    }
  },
  "util": {
    "captcha": {
      "length": 4,
      "width": 400,
      "height": 160,
      "redis": {
        "key_prefix": "captcha:"
      }
    }
  },
  "ai": {
    "models": [
      {
        "provider": "local",
        "api_key": "ollama",
        "endpoint": "http://localhost:11434/v1/chat/completions",
        "model_name": "gemma3:12b",
        "temperature": 0.7,
        "timeout": 90,
        "active": true,
        "description": "Ollama model",
        "rpm": 10,
        "weight": 100
      }
    ],
    "selector": {
      "load_models_interval": 5,
      "update_weight_interval": 2
    }
