      "role": "admin"
    }
  },
  "storage": {
    "db": {
      "debug": true,
      "auto_migrate": true,
      "dsn": "root:root@tcp(127.0.0.1:3306)/goinkblog?charset=utf8mb4&parseTime=True&loc=Local",
