{
  "middleware": {
    "cors": {
      "enable": false,
      "allow_origins": ["*"],
      "allow_methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS"],
      "allow_headers": ["*"],
      "max_age": 86400,
      "allow_wildcard": true,
      "allow_web_sockets": true,
      "allow_files": true
    },
    "auth": {
      "skipped_path_prefixes": [],
      "signing_method": "HS512",
      "signing_key": "fqe@#$%^&*()_+",
      "old_signing_key": "",
      "expired": 86400,
      "store": {
        "delimiter": ":"
      }
    },
    "trace": {
      "request_header_key": "X-Request-Id",
      "response_trace_key": "X-Trace-Id"
    },
    "logger": {
      "max_output_request_body_len": 4096,
      "max_output_response_body_len": 4096
    },
    "copy_body": {
      "max_content_len": 67108864
    },
    "recovery": {
      "skip": 2
    },
    "rate_limiter": {
      "enable": true,
      "skipped_path_prefixes": [],
      "max_request_per_ip_minute": 20,
      "max_request_per_user_second": 10,
      "max_ai_request_per_user_hour": 5
    },
    "casbin": {
      "skipped_path_prefixes": [],
      "model_file": "rbac_model.conf",
      "policy_file": "rbac_policy.csv"
    }
  }
} 